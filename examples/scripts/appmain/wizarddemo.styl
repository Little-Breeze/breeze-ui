
.dropdown-arrow-inverse {
  border-bottom-color: #ff6c03 !important;
  border-top-color: #ff6c03 !important;
}
.corners {
  border-radius: 3px !important;
}
.left-corners {
  border-radius: 3px 0 0 3px !important;
}
.right-corners {
  border-radius: 0 3px 3px 0 !important;
}
.shadow {
  box-shadow: 0px 1px 0px 0px #CCCCCC;
}
.find-pwd-container .find-pwd-content {
  position: relative;
  border: 1px solid #E4E4E4;
  height: 550px;
  background: #FFFFFF;
  // margin: 60px auto;
  padding: 0 230px;
}
.find-pwd-container .find-pwd-content .btn-primary {
  color: #FFF;
  background-color: #ff6c03;
}
.find-pwd-container .find-pwd-content .btn {
  border: none;
}
.find-pwd-container .find-pwd-content .btn.disabled,
.find-pwd-container .find-pwd-content .btn[disabled],
.find-pwd-container .find-pwd-content fieldset[disabled] .btn {
  background-color: #E4E4E4;
  color: white;
  border: none;
}
.find-pwd-container .find-pwd-content header {
  font-size: 18px;
  padding: 25px 0 26px 40px;
  line-height: 1;
  color: #333333;
  border-bottom: 1px solid #E4E4E4;
}
.find-pwd-container .find-pwd-content .btn.disabled,
.find-pwd-container .find-pwd-content .btn[disabled],
.find-pwd-container .find-pwd-content fieldset[disabled] .btn {
  opacity: .5;
  -webkit-opacity: .5;
  -moz-opacity: .5;
  -khtml-opacity: .5;
  // filter: alpha(opacity=50);
  color: white;
}
.find-pwd-container .find-pwd-content .common-wizard .modal-title {
  display: none;
}
.find-pwd-container .find-pwd-content .common-wizard .modal-header {
  border: 0px;
}
.find-pwd-container .find-pwd-content .common-wizard .modal-header button {
  display: none;
}
.find-pwd-container .find-pwd-content .common-wizard .steps-indicator li a {
  cursor: default;
  font-size: 16px;
  color: #E4E4E4;
}
.find-pwd-container .find-pwd-content .common-wizard .steps-indicator li a:before {
  height: 26px;
  width: 26px;
  background-color: #E4E4E4;
}
.find-pwd-container .find-pwd-content .common-wizard .steps-indicator li a .step-num {
  font-weight: normal;
}
.find-pwd-container .find-pwd-content .common-wizard .steps-indicator li.current a {
  color: #00BC8D;
  font-weight: 600;
}
.find-pwd-container .find-pwd-content .common-wizard .steps-indicator li.current a:before {
  background-color: #00BC8D;
}
.find-pwd-container .find-pwd-content .common-wizard .steps-indicator:before {
  height: 3px;
  background-color: #E4E4E4;
}
.find-pwd-container .find-pwd-content .common-wizard li.done a {
  color: #CDEAE3;
}
.find-pwd-container .find-pwd-content .common-wizard li.done a:before {
  background-color: #CDEAE3;
}
.find-pwd-container .find-pwd-content .common-wizard li.done span {
  color: #FFFFFF;
}
.find-pwd-container .find-pwd-content .common-wizard li.done span:after {
  top: 10px;
  background-color: #CDEAE3;
  content: '';
  position: absolute;
  height: 3px;
  width: 155px;
  left: 18px;
}
.find-pwd-container .find-pwd-content .common-wizard li.editing a {
  color: #00BC8D;
  font-weight: 600;
}
.find-pwd-container .find-pwd-content .common-wizard li.editing a:before {
  background-color: #00BC8D;
}
.find-pwd-container .find-pwd-content .common-wizard li.editing a .step-num {
  font-weight: normal;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .modal-header {
  padding-top: 34px;
}
.steps .wz-step {
  display: none;
}
.steps .current {
  display: block;
}
.find-pwd-container .find-pwd-content .password-reset-wizard ul {
  width: 723px;
  margin-left: auto;
  margin-right: auto;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .steps-indicator li {
  padding-left: 10px;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .steps-indicator li a > .step-num {
  left: 90px;
  font-size: 22px;
  top: -28px;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .steps-indicator li:first-child .step-num {
  left: 91px;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .steps-indicator li:first-child + li .step-num {
  left: 89px;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .steps-indicator li + li + li + li a > span.step-num {
  width: 0;
}
.find-pwd-container .find-pwd-content .password-reset-wizard .steps-indicator li + li + li + li a > span.step-num:before {
  content: '';
  width: 15px;
  height: 22px;
  display: block;
  background-repeat: no-repeat;
  background-position: center;
  position: relative;
  left: -2px;
}
.find-pwd-container .find-pwd-content .code-verify-row {
  width: 320px;
  margin: 0 auto;
}
.find-pwd-container .find-pwd-content .code-verify-row .col-xs-7 {
  width: 180px;
  padding: 0;
}
.find-pwd-container .find-pwd-content .code-verify-row .col-xs-5 {
  width: 140px;
  padding-left: 10px;
  padding-right: 0;
}
.find-pwd-container .find-pwd-content .code-verify-row .col-xs-5 button {
  font-size: 14px;
}
.find-pwd-container .find-pwd-content .input-wrapper {
  text-align: center;
}
.find-pwd-container .find-pwd-content .input-wrapper input {
  height: 44px;
  border: 1px solid #CCCCCC;
  border-radius: 4px;
  padding-left: 12px;
}
.find-pwd-container .find-pwd-content .input-wrapper input:focus {
  outline: 0;
  border-color: #00BC8D;
}
.find-pwd-container .find-pwd-content .button-wrapper {
  text-align: center;
}
.find-pwd-container .find-pwd-content .button-wrapper button,
.find-pwd-container .find-pwd-content .button-wrapper a {
  width: 320px;
  height: 44px;
  font-size: 16px;
}
.find-pwd-container .find-pwd-content .button-wrapper button {
  line-height: 17px;
}
.find-pwd-container .find-pwd-content .button-wrapper a {
  padding: 0;
  line-height: 44px;
}
.find-pwd-container .find-pwd-content .alert-msg {
  height: 60px;
  color: #D70C18;
  text-align: center;
  line-height: 60px;
}
.find-pwd-container .find-pwd-content .info-tip {
  color: #333333;
  line-height: 18px;
  width: 320px;
  margin: 25px auto 12px;
}
.find-pwd-container .find-pwd-content .first-step-form .input-phone-email {
  width: 320px;
  margin-top: 25px;
}
.find-pwd-container .find-pwd-content .first-step-form .input-code {
  width: 180px;
  margin-top: 12px;
}
.find-pwd-container .find-pwd-content .first-step-form .captcha-line {
  margin-top: 12px;
}
.find-pwd-container .find-pwd-content .first-step-form .captcha-line img {
  height: 44px;
  width: 86px;
  border: 1px solid #CCCCCC;
  border-right: 0;
  border-radius: 4px;
  border-bottom-right-radius: 0;
  border-top-right-radius: 0;
}
.find-pwd-container .find-pwd-content .first-step-form .captcha-line a {
  height: 44px;
  width: 44px;
  padding: 12px 5px 0 13px;
  border-bottom-left-radius: 0;
  border-top-left-radius: 0;
}
.find-pwd-container .find-pwd-content .first-step-form .captcha-line a span {
  font-size: 21px;
}
.find-pwd-container .find-pwd-content .second-step-form .input-verifyCode {
  width: 180px;
}
.find-pwd-container .find-pwd-content .second-step-form .send-code-btn {
  width: 130px;
  height: 44px;
  border: 1px solid #00BC8D;
  background-color: #FFFFFF;
  color: #00BC8D;
}
.find-pwd-container .find-pwd-content .second-step-form .send-code-btn[disabled] {
  border-color: #E4E4E4;
  background-color: #E4E4E4;
  color: #FFFFFF;
  padding-left: 8px;
}
.find-pwd-container .find-pwd-content .choose-other-type {
  width: 320px;
  margin: 10px auto 0;
  line-height: 20px;
  text-align: left;
  color: #ff6c03;
  cursor: pointer;
}
.find-pwd-container .find-pwd-content .choose-other-type a {
  font-size: 14px;
}
.find-pwd-container .find-pwd-content .third-step-form .input-pwd {
  width: 320px;
}
.find-pwd-container .find-pwd-content .third-step-form .confirm-pwd {
  margin-top: 12px;
}
.find-pwd-container .find-pwd-content .last-step {
  text-align: center;
  color: #333333;
}
.find-pwd-container .find-pwd-content .last-step .sl-icon-check {
  font-size: 78px;
  color: #00BC8D;
  line-height: 78px;
  margin-top: 25px;
}
.find-pwd-container .find-pwd-content .last-step .reset-successful {
  font-size: 18px;
  line-height: 1;
  margin: 21px 0 0px;
}
.find-pwd-container .find-pwd-content .last-step a {
  margin-top: 60px;
}
.find-pwd-container .find-pwd-content .dr-geetest-mask {
  position: absolute;
  text-align: center;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0,0,0,0.8);
  z-index: 1000;
}
.find-pwd-container .find-pwd-content .dr-geetest-mask>div {
  display: inline-block;
  vertical-align: middle;
}
.find-pwd-container .find-pwd-content .dr-geetest-mask:after {
  content: "";
  width: 0;
  font-size: 0;
  height: 100%;
  display: inline-block;
  vertical-align: middle;
}
